---
import { storyblokEditable } from '@storyblok/astro';
import TopAuthors from './TopAuthors.astro';

const { blok } = Astro.props;
---

<div {...storyblokEditable(blok)}>
  <div class='p-4'>
    <div class='flex flex-col w-fit'>
      <div>
        <TopAuthors blok={blok} />
      </div>
    </div>
  </div>
</div>
